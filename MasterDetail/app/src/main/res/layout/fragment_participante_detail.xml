<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:id="@+id/participante_detail_container"
    android:layout_width="match_parent" android:layout_height="match_parent"

    tools:ignore="MergeRootFrame">

    <TextView
        android:id="@+id/participante_detail"
        style="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:text="hel"
        android:textColor="#ff000000"
        android:textIsSelectable="true"
        android:textSize="35sp"
        tools:context=".ParticipanteDetailFragment" />

    <TextView
        android:id="@+id/participante_detailA"
        style="?android:attr/textAppearanceLarge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/participante_detail"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:padding="16dp"
        android:text="hela"
        android:textIsSelectable="true"
        tools:context=".ParticipanteDetailFragment" />
</RelativeLayout>